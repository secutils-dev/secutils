{
  "name": "secutils-webui",
  "private": true,
  "type": "module",
  "author": {
    "name": "Secutils.dev",
    "email": "dev@secutils.dev"
  },
  "version": "1.0.0-beta.2",
  "alias": {
    "process": "process/browser.js"
  },
  "targets": {
    "src": {
      "source": [
        "src/index.html",
        "src/favicon.ico",
        "src/tools/monaco/*"
      ],
      "distDir": "./dist",
      "engines": {
        "browsers": [
          "last 2 versions",
          "Firefox ESR",
          "not dead"
        ]
      }
    },
    "retrack": {
      "source": "./retrack/*.ts",
      "distDir": "./dist/retrack",
      "outputFormat": "esmodule",
      "sourceMap": false,
      "optimize": true,
      "engines": {
        "node": "22.x"
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.28.6",
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "^112.3.0",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@monaco-editor/react": "^4.7.0",
    "@ory/kratos-client-fetch": "^25.4.0",
    "moment": "^2.30.1",
    "monaco-editor": "^0.55.1",
    "nanoid": "^5.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.13.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@parcel/compressor-brotli": "^2.16.4",
    "@parcel/compressor-gzip": "^2.16.4",
    "@parcel/config-default": "^2.16.4",
    "@parcel/core": "^2.16.4",
    "@parcel/packager-raw-url": "^2.16.4",
    "@parcel/reporter-bundle-buddy": "^2.16.4",
    "@parcel/transformer-image": "^2.16.4",
    "@parcel/transformer-js": "^2.16.4",
    "@parcel/transformer-raw": "^2.16.4",
    "@parcel/transformer-typescript-tsc": "^2.16.4",
    "@parcel/transformer-webmanifest": "^2.16.4",
    "@parcel/validator-typescript": "^2.16.4",
    "@types/node": "^22.19.11",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "baseline-browser-mapping": "^2.9.19",
    "buffer": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "parcel": "^2.16.4",
    "playwright-core": "1.52.0",
    "prettier": "^3.8.1",
    "process": "^0.11.10",
    "svgo": "^4.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "url": "^0.11.4"
  },
  "optionalDependencies": {
    "@lmdb/lmdb-linux-arm64": "^3.5.1",
    "@parcel/rust-linux-arm64-musl": "^2.16.4",
    "@parcel/watcher-linux-arm64-musl": "^2.5.6",
    "@parcel/watcher-linux-x64-glibc": "^2.5.6",
    "@swc/core-linux-arm64-musl": "^1.15.11",
    "lightningcss-linux-arm64-musl": "^1.31.1",
    "lightningcss-linux-x64-gnu": "^1.31.1"
  },
  "overrides": {
    "typescript": "^5.9.3"
  },
  "scripts": {
    "prebuild": "eslint .",
    "build": "parcel build --no-source-maps",
    "watch": "parcel --port 7171",
    "perf": "parcel build --reporter @parcel/reporter-bundle-buddy",
    "lint": "eslint . --fix",
    "clean": "rm -rf dist .parcel-cache"
  }
}
